%h2 Sign in
= semantic_form_for(resource, :as => resource_name, :url => session_path(resource_name)) do |f|
  = f.inputs do
    = f.input :email
    = f.input :password
    - if devise_mapping.rememberable?
      = f.check_box :remember_me
      = f.label :remember_me
  = f.buttons do
    = f.commit_button "Sign in"

%br

= render :partial => "devise/shared/links"
